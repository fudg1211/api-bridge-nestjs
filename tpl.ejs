<% if(arg==='wx'){%>import { request } from 'xb-wx-request';<%}%>
<% if(arg!=='wx'){%>import { Request as request } from '@go/go-request';<%}%>

<% paths.forEach(function(path){%>
export async function <%=path.post.operationId%>(<% if(path.requestSchema){%>data<%=path.requestRequired%>: <%= path.requestSchema%><%}%>):Promise<<%= path.responseSchema%>>{
  return await request<<%= path.responseSchema%>>('<%= path.path%>'<% if(path.requestSchema){%>,data<%}%>);
}

export type <%=path.post.operationId%>_VO = <%= path.responseVO%>

<%})%>