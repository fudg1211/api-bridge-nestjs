<% if(arg==='wx'){%>import { request } from 'xb-wx-request';<%}%>
<% if(arg!=='wx'){%>import { Request as request } from '@go/go-request';<%}%>
import {paths} from '../../api';
type Post = <%- PostSchema%>;
export type <%- to%> = <%- requestSchema%>;
export type <%- res%> = <%- responseSchema%>;
<% if(vo){%>
//只有接口名称包含List才会有此导出 apiName 符合此规则 /List$/
export type <%- vo%> = <%- responseSchemaVo%>;
<%}%>
export async function <%-apiName%>(<% if(requestSchema){%>data: <%- to%><%}%>):Promise<<%- res%>>{
	return await request<<%- res%>>('<%- api%>'<% if(requestSchema){%>,data<%}%>);
}
